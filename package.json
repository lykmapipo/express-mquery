{
  "name": "express-mquery",
  "version": "1.2.14",
  "description": "Expose mongoose query API through HTTP request",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --require @babel/register test/**/*.spec.js",
    "test:unit": "NODE_ENV=test mocha --require @babel/register test/unit/**/*.spec.js",
    "test:integration": "npm run pretest && NODE_ENV=test mocha --require @babel/register test/integration/**/*.spec.js",
    "dev": "NODE_ENV=development BASE_PATH=./examples babel-node ./examples/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lykmapipo/express-mquery.git"
  },
  "author": "Lally Elias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/express-mquery/issues"
  },
  "homepage": "https://github.com/lykmapipo/express-mquery",
  "contributors": [
    {
      "name": "lykmapipo",
      "github": "https://github.com/lykmapipo"
    }
  ],
  "keywords": [
    "express",
    "mongoose",
    "query",
    "connect",
    "http",
    "blueprint",
    "api",
    "rest",
    "search",
    "filter",
    "find",
    "sort",
    "skip",
    "distinct",
    "limit",
    "query",
    "paginate",
    "middleware",
    "mongodb",
    "mquery",
    "jsonapi",
    "includes",
    "fields",
    "page",
    "stale",
    "fresh",
    "cache",
    "modified",
    "if-modified-since"
  ],
  "dependencies": {
    "async": ">=3.2.0",
    "auto-parse": ">=1.8.0",
    "express": ">=4.17.1",
    "lodash": ">=4.17.20"
  },
  "peerDependencies": {},
  "devDependencies": {
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "body-parser": ">=1.19.0",
    "chai": "^4.3.0",
    "dot-prop": "^6.0.1",
    "faker": "^5.4.0",
    "eslint": "^7.22.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/travis-cli": "^12.0.1",
    "commitizen": "^4.2.3",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "handlebars": "^4.7.7",
    "lodash.template": "^4.5.0",
    "minimist": "^1.2.5",
    "mocha": "^8.3.0",
    "qs": "^6.9.6",
    "rollup": "^2.50.6",
    "shelljs": "^0.8.4",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "supertest": "^6.1.3",
    "websocket-extensions": "^0.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
